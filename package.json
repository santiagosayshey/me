{
  "name": "me",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run manifest:watch\"",
    "build": "npm run manifest:generate && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json",
    "manifest:generate": "tsx src/navigation/pageNav/utils/manifest.ts",
    "manifest:watch": "tsx src/navigation/pageNav/utils/manifest.ts --watch"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "concurrently": "^9.2.1",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "katex": "^0.16.22",
    "lucide-svelte": "^0.542.0",
    "shiki": "^3.12.0",
    "simple-icons": "^15.12.0",
    "svelte-routing": "^2.13.0",
    "tailwindcss": "^4.1.12"
  }
}
